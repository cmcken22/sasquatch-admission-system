<form id="add-student-form">
        <h2>Add new Program</h2>
        <div class="row">
            <div class="large-12 columns">
                <label>Program Name
                  {{input value=programName  id="programName" type="text"}}
                  Code
                  {{input value=code  id="code" type="text"}}
                  Subcode
                  {{input value=subCode  id="subCode" type="text"}}
                </label>
            </div>
        </div>
    </form>
    <br>
    <a href="#" class="button radius" {{action 'addProgram' programName}}>Save</a>
    
    {{!--<a href="#" class="button radius" {{action 'addGender'}}>Testing Add Gender</a>--}}
    <a href="#" class="button radius" >Cancel</a>
